@^(https?://|www\.)      = $BROWSER
@^magnet:                = transmission-qt
@^steam:                 = steam
image/                   = sxiv
video/                   = mplayer
audio/                   = mplayer?:urxvtc
text/html                = $BROWSER
text/                    = $EDITOR?:urxvtc
application/pdf          = zathura
inode/directory          = ranger?:urxvtc
